    .player-list-detail ul {
        margin: 5px 16px;
    }

        .player-list-detail ul li {
            border-bottom: 1px solid rgba(255, 255, 255, 0.4);
            padding-bottom: 4px;
            margin-bottom: 4px;
        }

            .player-list-detail ul li:last-child {
                border: none;
            }

        .player-list-detail ul a {
            color: #b8b8b8;
            display: flex;
            justify-content: space-between;
