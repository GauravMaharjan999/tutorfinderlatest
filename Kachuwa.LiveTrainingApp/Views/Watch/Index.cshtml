                }

                .player-course-info .player-course-list .course-list-detail li.active {
                    color: #fff;
                    background: #4d4d4d;
                    padding: 2px;
                }

                    .player-course-info .player-course-list .course-list-detail li.active::before {
                        font-family: "Material Icons";
                        content: "play_arrow";
                        position: absolute;
                        top: 3px;
                        left: -18px;
                        width: 15px;
                        height: 15px;
                        background-color: Transparent;
                        background-repeat: no-repeat;
                        border: none;
                        cursor: pointer;
                        color: #d9d9d9;
                        padding: 0px;
                        cursor: pointer;
                        outline: none;
                        font-size: 15px;
