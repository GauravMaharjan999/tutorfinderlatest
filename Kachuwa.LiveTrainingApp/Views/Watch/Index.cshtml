        height: calc(100vh - 84px);
        overflow-y: scroll;
    }

        /*@@media (max-width: 1024px) {
            .player-course-info {
                width: 100%;
            }
        }*/

        .player-course-info h4 {
            background: #464646;
            padding: 10px;
            margin: 0;
        }

        .player-course-info .player-course-info-items {
