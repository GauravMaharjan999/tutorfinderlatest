    .hide {
        display: none;
    }

    .disable {
        pointer-events: none;
    }

    .unselectable {
        -webkit-touch-callout: none;
        -webkit-user-select: none;
        -khtml-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
    }

    :-webkit-full-screen {
        width: 100%;
        top: 0;
        z-index: -100;
        background: none;
    }

    :-ms-fullscreen {
        width: auto;
        height: auto;
        margin: auto;
    }

    .time-display, .duration-display {
        padding: 11px;
        color: white;
        font-weight: normal;
        font-size: 0.9em;
        font-family: "Open Sans", "Avenir", sans-serif;
    }

    .time-display {
        float: left;
    }

    .duration-display {
        float: right;
    }

    .live-icon {
        cursor: pointer;
    }

        .live-icon.live {
            color: #eb5757 !important;
            pointer-events: none;
            cursor: default;
        }

    .btn-play-pause {
        float: left;
        padding: 9px 10px;
        cursor: pointer;
    }

    .control-icon-layout {
        float: right;
        padding: 9px 10px;
        cursor: pointer;
        position: relative;
    }

    .btn-fullscreen {
        margin-right: 10px;
    }

    .volumebar {
        float: right;
        width: 70px;
    }

    .full-width .fullscreen {
        height: 100% !important;
    }

        .full-width .fullscreen video {
            width: 100%;
            height: 100% !important;
            margin: 0;
            position: relative;
            top: 0;
            -ms-transform: translateY(0%);
            transform: translateY(0%);
        }

    .videoContainer {
        width: 100%;
        height: 100vh;
        position: relative;
        overflow: hidden;
    }

        /*@@media (max-width: 1024px) {
            .videoContainer {
                width: 100%;
                height: 50vh;
            }
        }*/

        .videoContainer.fullscreen {
            height: 100% !important;
        }

            .videoContainer.fullscreen video {
                width: 100%;
                height: 100vh;
                margin: 0;
                position: relative;
                top: 0;
                -ms-transform: translateY(0%);
                transform: translateY(0%);
            }

        .videoContainer button[data-state="play"], .videoContainer button[data-state="pause"] {
            position: absolute;
            top: 50%;
            left: 50%;
            width: 50px;
            height: 50px;
            margin: -25px -25px;
            background-color: Transparent;
            background-repeat: no-repeat;
            border: none;
            cursor: pointer;
            overflow: hidden;
            transform: scale(2.1);
            background: #5BA536;
            color: white;
            font-size: 40px;
            border-radius: 50%;
            padding: 0px;
            -webkit-font-smoothing: antialiased;
            font-family: "Material Icons";
            -webkit-font-feature-settings: "liga" 1;
            -moz-font-feature-settings: "liga" 1;
            font-feature-settings: "liga" 1;
            z-index: 2147483646;
            cursor: pointer;
            outline: none;
            transition-property: all;
            transition-delay: 4s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
            opacity: 0;
        }

            .videoContainer button[data-state="play"]:before {
                content: "play_arrow";
            }

            .videoContainer button[data-state="pause"]:before {
                content: "pause";
            }

        .videoContainer .course-header {
            position: absolute;
            top: 0;
            width: 100%;
            background: #000ab;
            min-height: 100px;
            opacity: 0;
            z-index: 2147483646;
            width: 100%;
            -webkit-box-shadow: 0px 0px 70px 0px transparent;
            -moz-box-shadow: 0px 0px 70px 0px transparent;
            box-shadow: 0px 0px 70px 0px transparent;
            background: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.7));
            /* -webkit-transition: 1.3s ease-in-out;
         */
            /* transition: 1.3s ease-in-out;
         */
            transform: translateY(-40px);
            transition-property: all;
            transition-delay: 4s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
        }

            .videoContainer .course-header .course-header-content {
                float: left;
            }

                .videoContainer .course-header .course-header-content .course-header-title {
                    font-size: 24px;
                    padding: 14px 10px;
                }

                .videoContainer .course-header .course-header-content .course-header-overview {
                    font-size: 16px;
                    padding: 0px 10px;
                }

        .videoContainer .toggle-sidebar {
            float: right;
        }

            .videoContainer .toggle-sidebar button[data-state="next"], .videoContainer .toggle-sidebar button[data-state="prev"] {
                width: 50px;
                height: 50px;
                cursor: pointer;
                font-size: 40px;
                padding: 0;
                margin: 0;
                -webkit-font-smoothing: antialiased;
                font-family: "Material Icons";
                -webkit-font-feature-settings: "liga" 1;
                -moz-font-feature-settings: "liga" 1;
                font-feature-settings: "liga" 1;
                background: #00b3ff;
            }

                .videoContainer .toggle-sidebar button[data-state="next"]:before {
                    content: "navigate_next";
                }

                .videoContainer .toggle-sidebar button[data-state="prev"]:before {
                    content: "navigate_before";
                }

        .videoContainer:hover .video-controller {
            opacity: 1;
            transform: translateY(1px);
            transition-property: all;
            transition-delay: 0s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
        }

        .videoContainer:hover .course-header {
            opacity: 1;
            transform: translateY(1px);
            transition-property: all;
            transition-delay: 0s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
        }

        .videoContainer:hover button {
            opacity: 1;
            transition-property: all;
            transition-delay: 0s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
        }

        .videoContainer button[data-state="play"] {
            opacity: 1;
            transition-property: all;
            transition-delay: 0s;
            transition-duration: 2s;
            transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
        }

            .videoContainer button[data-state="play"] .videoContainer {
                opacity: 1;
            }

                .videoContainer button[data-state="play"].videoContainer button {
                    opacity: 1;
                }

    .video-controller {
        opacity: 0;
        min-height: 35px;
        z-index: 2147483646;
        position: absolute;
        bottom: 0px;
        width: 100%;
        -webkit-box-shadow: 0px 0px 70px 0px transparent;
        -moz-box-shadow: 0px 0px 70px 0px transparent;
        box-shadow: 0px 0px 70px 0px transparent;
        background: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.7));
        /* -webkit-transition: 1.3s ease-in-out;
         */
        /* transition: 1.3s ease-in-out;
         */
        transform: translateY(50px);
        transition-property: all;
        transition-delay: 4s;
        transition-duration: 2s;
        transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
    }

    .videoContainer button:hover {
        background: #fff;
        -webkit-box-shadow: 0 0 51px 8px #5BA536;
        box-shadow: 0 0 51px 8px #5BA536;
        color: #5BA536;
    }

    .video-controller-fullscreen {
        position: fixed;
        z-index: 2147483647;
        width: 100%;
        bottom: 0;
        left: 0;
    }

    .hd:before {
        content: "hd";
        font-family: "Material Icons";
        position: absolute;
        margin-left: -7px;
        background: #757575;
        color: white;
        font-size: 14px;
        margin-top: -2px;
        position: absolute;
    }

    .icon-play, .icon-pause, .icon-caption, .icon-mute-off, .icon-mute-on, .icon-fullscreen-enter, .icon-fullscreen-exit, .icon-tracks, .icon-bitrate {
        font-size: 20px;
        color: #fff;
        text-shadow: none;
        -webkit-font-smoothing: antialiased;
        font-family: "Material Icons";
        -webkit-font-feature-settings: "liga" 1;
        -moz-font-feature-settings: "liga" 1;
        font-feature-settings: "liga" 1;
    }

        .icon-fullscreen-enter:before {
            content: "fullscreen";
        }

        .icon-fullscreen-exit:before {
            content: "fullscreen_exit";
        }

        .icon-play:before {
            content: "play_arrow";
        }

        .icon-pause:before {
            content: "pause";
        }

        .icon-mute-on:before {
            content: "volume_off";
        }

        .icon-mute-off:before {
            content: "volume_up";
        }

        .icon-caption:before {
            content: "closed_caption";
        }

        .icon-bitrate:before {
            content: "settings";
        }

        .icon-tracks:before {
            content: "audiotrack";
        }

    .seekContainer {
        display: flex;
        overflow: auto;
        overflow-y: hidden;
        overflow-x: hidden;
        width: 98%;
        margin: 0 auto;
    }

