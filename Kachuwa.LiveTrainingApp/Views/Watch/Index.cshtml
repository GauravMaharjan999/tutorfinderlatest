    }

        input[type="range"] {
            outline: none;
            /* no focus outline */
            background: #fff;
            border: 0;
            border-radius: 100%;
            box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), 0 0 0 1px rgba(35, 41, 47, 0.2);
            height: 13px;
            position: relative;
            transition: all 0.2s ease;
            width: 13px;
        }

            input[type="range"]::-moz-focus-outer {
                border: 0;
            }

            input[type="range"]::-moz-range-track {
                border: inherit;
                background: transparent;
            }

            input[type="range"]::-ms-track {
                border: inherit;
                color: transparent;
                /* don't drawn vertical reference line */
                background: transparent;
            }

            input[type="range"]::-ms-fill-lower, input[type="range"]::-ms-fill-upper {
                background: transparent;
            }

            input[type="range"]::-ms-tooltip {
                display: none;
            }

            input[type="range"]::-webkit-slider-thumb {
                -webkit-appearance: none;
                width: 15px;
                height: 8px;
                border: none;
                border-radius: 2px;
                background-color: #00b3ff;
            }

            input[type="range"]::-moz-range-thumb, input[type="range"]::-ms-thumb {
                width: 15px;
                height: 8px;
                border: none;
                border-radius: 2px;
                background-color: #00b3ff;
            }
    /* thumb */
    .thumbnail-container {
        position: absolute;
        text-align: center;
    }

    .thumbnail-elem {
        position: relative;
        box-shadow: 0px 0px 0.9em #000;
        transform-origin: center bottom;
    }

    .thumbnail-time-label {
        position: relative;
        bottom: 1.8em;
        display: table;
        margin: 0 auto;
        padding: 2px 5px 2px 5px;
        color: #fff;
        background-color: rgba(0, 0, 0, 0.7);
        font-size: 12px;
        font-weight: bold;
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    }

    .seekbar-complete {
        width: 100%;
        height: 4px;
        margin-top: 10px;
        background: #f0efef 8f;
        background: rgba(240, 239, 239, 0.84);
        position: relative;
        overflow: hidden;
    }

    .seekbar-buffer {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 0%;
        height: 7px;
        background: #fff;
    }

    .seekbar-play {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 0%;
        height: 7px;
        background: #00b3ff;
    }

    input[type="range"] {
        -webkit-appearance: none;
        background: 0 0;
        border: 0;
        border-radius: 26px;
        color: #00b3ff;
        display: block;
        height: 19px;
        margin: 0;
        margin-top: 10px;
        padding: 0;
        transition: box-shadow 0.3s ease;
        width: 8%;
    }

        input[type="range"]::-webkit-slider-runnable-track {
            background: 0 0;
            border: 0;
            border-radius: 2.5px;
            height: 5px;
            transition: box-shadow 0.3s ease;
            -webkit-user-select: none;
            user-select: none;
            background-image: linear-gradient(to right, currentColor var(--value, 0), transparent var(--value, 0));
        }

        input[type="range"]::-webkit-slider-thumb {
            background: #fff;
            border: 0;
            border-radius: 100%;
            box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), 0 0 0 1px rgba(35, 41, 47, 0.2);
            height: 13px;
            position: relative;
            transition: all 0.2s ease;
            width: 13px;
            -webkit-appearance: none;
            margin-top: -4px;
        }

        input[type="range"]::-moz-range-track {
            background: 0 0;
            border: 0;
            border-radius: 2.5px;
            height: 5px;
            transition: box-shadow 0.3s ease;
            -moz-user-select: none;
            user-select: none;
        }

        input[type="range"]::-moz-range-thumb {
            background: #fff;
            border: 0;
            border-radius: 100%;
            box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), 0 0 0 1px rgba(35, 41, 47, 0.2);
            height: 13px;
            position: relative;
            transition: all 0.2s ease;
            width: 13px;
        }

        input[type="range"]::-moz-range-progress {
            background: currentColor;
            border-radius: 2.5px;
            height: 5px;
        }

        input[type="range"]::-ms-track {
            background: 0 0;
            border: 0;
            border-radius: 2.5px;
            height: 5px;
            transition: box-shadow 0.3s ease;
            -ms-user-select: none;
            user-select: none;
            color: transparent;
        }

        input[type="range"]::-ms-fill-upper {
            background: 0 0;
            border: 0;
            border-radius: 2.5px;
            height: 5px;
            transition: box-shadow 0.3s ease;
            -ms-user-select: none;
            user-select: none;
        }

        input[type="range"]::-ms-fill-lower {
            background: 0 0;
            border: 0;
            border-radius: 2.5px;
            height: 5px;
            transition: box-shadow 0.3s ease;
            -ms-user-select: none;
            user-select: none;
            background: currentColor;
        }

        input[type="range"]::-ms-thumb {
            background: #fff;
            border: 0;
            border-radius: 100%;
            box-shadow: 0 1px 1px rgba(0, 0, 0, 0.15), 0 0 0 1px rgba(35, 41, 47, 0.2);
            height: 13px;
            position: relative;
            transition: all 0.2s ease;
            width: 13px;
            margin-top: 0;
        }

        input[type="range"]::-ms-tooltip {
            display: none;
        }

        input[type="range"]:focus {
            outline: 0;
        }

        input[type="range"]::-moz-focus-outer {
            border: 0;
        }

    .menu-container {
        animation: plyr-popup 0.2s ease;
        background: rgba(255, 255, 255, 0.9);
        border-radius: 4px;
        bottom: 100%;
        box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15);
        color: #4a5764;
        font-size: 13px;
        margin-bottom: 10px;
        position: absolute;
        right: -3px;
        text-align: left;
        white-space: nowrap;
        z-index: 3;
    }

        .menu-container::after {
            border: 10px solid transparent;
            border-top-color: rgba(255, 255, 255, 0.9);
            content: "";
            height: 0;
