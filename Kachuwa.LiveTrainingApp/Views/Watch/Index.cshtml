<script>
    $(document).ready(function () {
        $("#dvchat").hide();
        $("#btnclosesidebar").hide();
        var ChatContent = "";
    //load messages or question of playing stream of user
    $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                 url: "/watch/getMessagesofUserbyStreamId/" + @ViewBag.StreamId,
                dataType: "json",
                data: {},
                success: function (response) {

                    if (response.Code == 200) {
                      
                        $.each(response.Data,
                            function (i, data) {
                                
                             ChatContent = ChatContent +'<ul>' +
                             '<li class="clearfix">'+
                                     '<div class="message-data align-right">' +
                                    ' <span class="message-data-time">' + $.datepicker.formatDate("dd-M-yy", new Date(data.AddedOn))+'</span> &nbsp; &nbsp;' +
                              '<span class="message-data-name">@User.Identity.Name</span> <i class="fa fa-circle me"></i>'+

                                     ' </div>' +
                                     ' <div class="message other-message float-right">' + data.Message+
                            '</div>'+
                      '  </li>'+
                      '</ul>'
                            });
                        $("#chathistory").append(ChatContent);
                    }
                    else {
                       
                    }
                }
            });
    });
    //opens ask  question to tutor sidebar
    $('#btnopensidebar').on('click', function () {

        $("#playercontainer").removeClass("player-container col-md-12").addClass('player-container col-md-9');
        $("#dvchat").show();
        $("#btnclosesidebar").show();
        $("#btnopensidebar").hide();
    });
    //closes ask question to tutor sidebar
    $('#btnclosesidebar').on('click', function () {

        $("#playercontainer").removeClass("player-container col-md-9").addClass('player-container col-md-12');
        $("#dvchat").hide();
        $("#btnopensidebar").show();
        $("#btnclosesidebar").hide();

    });



    $('#btnSendMessage').on('click', function () {

        var Message = $("#message-to-send").val();

        if (Message == "") {
            return false;
        }

        $.ajax({
            type: "POST",
            url: "watch/UpdateQuestionsForTutor?streamId=" +@ViewBag.StreamId+"&Message=" + Message,
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            //data: JSON.stringify(model),

            success: function (res) {

                if (res.Code == 200) {
                    var  ChatContent =  '<ul>' +
                             '<li class="clearfix">'+
                                     '<div class="message-data align-right">' +
                                    ' <span class="message-data-time">' + $.datepicker.formatDate("dd-M-yy", new Date())+'</span> &nbsp; &nbsp;' +
                              '<span class="message-data-name">@User.Identity.Name</span> <i class="fa fa-circle me"></i>'+
                                     ' </div>' +
                        ' <div class="message other-message float-right">' + Message+
                            '</div>'+
                      '  </li>'+
                        '</ul>'
                    $("#chathistory").append(ChatContent);
                    $("#message-to-send").val("");
                    $("#sendingmessageerror").text("");

                }

                else {
                    $("#sendingmessageerror").text("Sending Failed");
                }

            },
            error: function (res) {
                if (res.status == 401) {
                    alert("Error!!! Please Try Again Later!!! !!!");
                    return false;
                }
            }
        });
    });

</script>
